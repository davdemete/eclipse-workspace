	      public class Main {
	    	  public static void main(String[] args) {
	    	    int a = 10;
	    	    int b = 20;
	    	    int c = 30;
	    	    System.out.println("El valor de a es: " + a);
	    	    System.out.println("El valor de b es: " + b);
	    	    System.out.println("El valor de c es: " + c);
	    	    System.out.println("La suma de a, b y c es: " + (a + b + c));
	    	  }
	    	}
