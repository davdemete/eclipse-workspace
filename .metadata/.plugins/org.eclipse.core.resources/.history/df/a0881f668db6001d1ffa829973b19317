import java.util.Scanner;

public class MensajeSegunNumero {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Ingrese un n√∫mero: ");
        int numero = sc.nextInt();
        
        switch (numero) {
            case 1:
                System.out.println("Gestores 1.");
                break;
            case 2:
                System.out.println("Clientes 2.");
                break;
            case 3:
                System.out.println("Tranferencias 3.");
                break;
            default:
                System.out.println("Mensajes 4.");
                break;
        }
    }
}
