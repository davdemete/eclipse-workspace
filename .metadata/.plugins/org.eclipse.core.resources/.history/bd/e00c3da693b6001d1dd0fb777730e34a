
public class BucleFor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int edad3 = 1;
		// comenzando con i=edad, mientras que i sea menor que 18 y con un incremento de i
		de uno en uno
		for (int i=edad3; i<18; i++) {
		System.out.printf("%d", i).println();
		}
		System.out.printf("Programa finalizado --> edad: %d", edad3).println();
	
	for(int i=0; i<100; i--) {
		System.out.println("Hola");
		}
		System.out.println("AdiÃ³s");

